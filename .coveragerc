[run]
branch = True
source =
    backend/analyser/*
    backend/core/*
    backend/recorder/*
omit =
    backend/test/*
    backend/scratch/*

[report]
exclude_lines =
    if self.debug:
    pragma: no cover
    raise NotImplementedError
    if __name__ == .__main__.:
ignore_errors = True
omit =
    backend/test/*